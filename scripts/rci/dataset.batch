#!/bin/bash

#SBATCH --partition=gpu
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --gres=gpu:4
#SBATCH --exclusive
#SBATCH --mem-per-gpu=8G
#SBATCH --cpus-per-task=16
#SBATCH --time=01:00:00

id=$1
dataset=$2

srun --output=./logs/${id}/${dataset}/run.log \
python -u experiments/blueprint_mlflow.py --experiment=pelesjak-deep-db-experiments-v1 \
--cuda --dataset=$dataset --num_samples=8 --run_name=${id}_${dataset}


